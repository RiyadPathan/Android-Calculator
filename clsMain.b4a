End Sub

Sub cmdInverse_Click
	txtInput.Text = "1" / txtInput.Text
End Sub

Sub cmdEqual_Click
	str2 = (txtInput.Text)
	If str = "-" Then
		txtInput.Text = str1 - str2
	Else If str = "+" Then
		txtInput.Text = str1 + str2
	Else If str = "*" Then
		txtInput.Text = str1 * str2
	Else If str = "/" Then
		txtInput.Text = str1 / str2
	Else if str = "PowerOf" Then
		txtInput.Text = Power( str1 , str2 )
	End If
	EqualAction = True
End Sub

Sub cmdDivide_Click
	str1 = (txtInput.Text)
	txtInput.Text = ""
	str = cmdDivide.Text
End Sub

Sub cmdDecimal_Click
	If txtInput.Text = "0." Then
		txtInput.Text = "."
	Else If txtInput.Text = str2 Then
		txtInput.Text = "."
	Else If txtInput.Text = str Then
		txtInput.Text = "."
	Else
		If txtInput.Text.Contains(".") Then
		Else
			txtInput.Text = txtInput.Text & "."
		End If
	End If
End Sub

