Sub cmdSubtract_Click
	str1 = (txtInput.Text)
	txtInput.Text = ""
	str = cmdSubtract.Text
End Sub

Sub cmdSqrRoot_Click
	str1 = (txtInput.Text)
	str1 = Sqrt(str1)
	txtInput.Text = str1 
End Sub

Sub cmdSign_Click
	txtInput.Text = "-1" * txtInput.Text
End Sub

Sub cmdPowerOf_Click
	str1 = (txtInput.Text)
	txtInput.Text = ""
	str = "PowerOf"
End Sub

Sub cmdMultiply_Click
	str1 = (txtInput.Text)
	txtInput.Text = ""
